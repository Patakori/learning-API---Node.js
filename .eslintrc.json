{
  "env": {
      "es2020": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "airbnb-base",
      "prettier",
      "plugin:prettier/recommended",
      "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "prettier",
      "@typescript-eslint",
      "eslint-plugin-import-helpers"
  ],
  "rules": {
    "prettier/prettier": "error",
    "quotes": ["error", "double"]
  },
  "settings": {
      "import/resolver": {
          "typescript": {}
      }
  }
}